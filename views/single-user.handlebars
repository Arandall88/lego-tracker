<h1>{{user.username}}'s Lego Sets</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Set Name</th>
            <th scope="col">ID</th>
            <th scope="col">Number of Pieces</th>
            <th scope="col">Set Number</th>
            <th scope="col">Image URL</th>
        </tr>
    </thead>
    <tbody>

        {{#each legos}}
        <tr>
            <th scope="row">{{name}}</th>
            <td>{{id}}</td>
            <td>{{numberOfPieces}}</td>
            <td>{{setNumber}}</td>
            <td>{{imageURL}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>